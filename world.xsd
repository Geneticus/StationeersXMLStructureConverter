<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
  
	<!-- Root Element for WorldData -->
	<xs:element name="WorldData">
	<xs:complexType>
	  <xs:choice maxOccurs="unbounded">
		<xs:element name="Game" type="xs:string" minOccurs="0" />
		<xs:element name="GameVersion" type="xs:string" minOccurs="0" />
		<xs:element name="DateTime" type="xs:string" minOccurs="0" />
		<xs:element name="DaysPast" type="xs:int" minOccurs="0" />
		<xs:element name="WorldName" type="xs:string" minOccurs="0" />
		<xs:element name="Clouds" type="xs:string" minOccurs="0" />
		<xs:element name="RespawnDifficulty" type="xs:string" minOccurs="0" />
		<xs:element name="CurrentMission" type="xs:string" minOccurs="0" />
		<xs:element name="ActiveMissions" type="xs:string" minOccurs="0" maxOccurs="1" />
		<xs:element name="CompletedMissions" type="xs:string" minOccurs="0" maxOccurs="1" />
		<xs:element name="StationContacts">
		  <xs:complexType>
			<xs:sequence>
			  <!-- Add the StationContactData element -->
			  <xs:element name="StationContactData" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
				  <xs:sequence>
					<xs:element name="Angle" nillable="true" minOccurs="0">
					  <xs:complexType>
						<xs:sequence>
						  <xs:element name="x" type="xs:float" />
						  <xs:element name="y" type="xs:float" />
						  <xs:element name="z" type="xs:float" />
						</xs:sequence>
					  </xs:complexType>
					</xs:element>
					<xs:element name="ContactName" type="xs:string" minOccurs="0" />
					<xs:element name="ContactType" type="xs:string" minOccurs="0" />
					<xs:element name="Lifetime" type="xs:float" minOccurs="0" />
					<xs:element name="TradeItemData" type="xs:string" minOccurs="0" />
					<xs:element name="TraderInventorySelector" type="xs:string" minOccurs="0" />
					<xs:element name="OverallIndex" type="xs:int" minOccurs="0" />
					<xs:element name="VisitorCurrency" type="xs:int" minOccurs="0" />
					<xs:element name="ContactID" type="xs:int" minOccurs="0" />
				  </xs:sequence>
				</xs:complexType>
			  </xs:element>
			 </xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="ResearchKey" type="xs:string" minOccurs="0" />
		<xs:element name="UnlockedResearchHash" type="xs:string" minOccurs="0" />
		<xs:element name="OverallIndexOfContacts" type="xs:int" minOccurs="0" />
		<xs:element name="WorldType" type="xs:string" minOccurs="0" />
		<xs:element name="WorldSeed" type="xs:int" minOccurs="0" />
		<xs:element name="BedrockLevel" type="xs:int" minOccurs="0" />
		<xs:element name="SunTime" type="xs:string" minOccurs="0" />
		<xs:element name="Rooms" type="xs:string" minOccurs="0" maxOccurs="1" />
		<xs:element name="PipeNetworks">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="NetworkId" type="xs:int" maxOccurs="unbounded" minOccurs="0" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="CableNetworks">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="NetworkId" type="xs:int" maxOccurs="unbounded" minOccurs="0" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="ChuteNetworks">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="NetworkId" type="xs:int" maxOccurs="unbounded" minOccurs="0" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="RocketShuttleNetworks" type="xs:string" minOccurs="0" />
		<xs:element name="SpawnPoints" type="xs:string" minOccurs="0" />
		<xs:element name="Motherships" type="xs:string" minOccurs="0" />
		<xs:element name="Things">
			<xs:complexType>
			  <xs:sequence>
				<xs:element name="ThingSaveData" type="ThingSaveDataBase" maxOccurs="unbounded" />
			  </xs:sequence>
			</xs:complexType>
		  </xs:element>
		<xs:element name="ISpawnPoints" />
		<xs:element name="Atmospheres">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="AtmosphereSaveData" type="AtmosphereSaveData" maxOccurs="unbounded" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="UserInterface">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="OpenSlots" type="xs:string" minOccurs="0" />
			<xs:element name="SelectedButton" type="xs:int" />
			<xs:element name="ActiveHandSlot" type="xs:int" />
		  </xs:sequence>
		</xs:complexType>
	  </xs:element>
		<xs:element name="TileSystemData">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="Incidents" type="xs:string" minOccurs="0" />
			  <xs:element name="VoxelFeatures" type="xs:string" minOccurs="0" />
			  <xs:element name="EnteredTiles" type="xs:string" minOccurs="0" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="WeatherManagerData">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="IsWeatherEventRunning" type="xs:boolean" minOccurs="0" />
			  <xs:element name="IsWeatherEventScheduled" type="xs:boolean" minOccurs="0" />
			  <xs:element name="WeatherStartTime" type="xs:float" minOccurs="0" />
			  <xs:element name="WeatherEventLength" type="xs:float" minOccurs="0" />
			  <xs:element name="DaysSinceLastWeatherEvent" type="xs:int" minOccurs="0" />
			  <xs:element name="LastEventCoolDown" type="xs:int" minOccurs="0" />
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="OriginPosition" type="PositionType" minOccurs="0" />
		<xs:element name="ResearchPodData">
		  <xs:complexType>
			<xs:sequence>
			  <xs:element name="ResearchPod" maxOccurs="unbounded">
				<xs:complexType>
				  <xs:sequence>
					<xs:element name="ResearchType" type="xs:string" />
					<xs:element name="Quantity" type="xs:int" />
				  </xs:sequence>
				</xs:complexType>
			  </xs:element>
			</xs:sequence>
		  </xs:complexType>
		</xs:element>        
	  </xs:choice>
	</xs:complexType>
	</xs:element>

	<!-- Base ThingSaveData Type -->
	<xs:complexType name="ThingSaveDataBase" abstract="true">
	 <xs:choice maxOccurs="unbounded">
		<xs:element name="ReferenceId" type="xs:int" />
		<xs:element name="PrefabName" type="xs:string" />
		<xs:element name="CustomName" type="xs:string" />
		<xs:element name="WorldPosition" type="WorldPositionType" minOccurs="1" />
		<xs:element name="WorldRotation" type="WorldRotationType" />
		<xs:element name="eulerAngles" type="EulerAnglesType" minOccurs="0" />
		<xs:element name="States" minOccurs="0">
		  <xs:complexType>
			<xs:sequence minOccurs="0">
			  <xs:element name="State" maxOccurs="unbounded" minOccurs="0">
				<xs:complexType>
				  <xs:sequence>
					<xs:element name="StateName" type="xs:string" />
					<xs:element name="State" type="xs:string" />
				  </xs:sequence>
				</xs:complexType>
			  </xs:element>
			</xs:sequence>
		  </xs:complexType>
		</xs:element>
		<xs:element name="Reagents" type="ReagentsType" minOccurs="0" nillable="true" />
		<xs:element name="Indestructable" type="xs:boolean" minOccurs="0" nillable="true" />
		<xs:element name="DamageState" type="DamageStateType" minOccurs="0" nillable="true" />
		<xs:element name="IsCustomName" type="xs:boolean" minOccurs="0" nillable="true" />
		<xs:element name="CustomColorIndex" type="xs:int" minOccurs="0" nillable="true" />
		<xs:element name="OwnerSteamId" type="xs:long" minOccurs="0" nillable="true" />
	</xs:choice>
	</xs:complexType>

